public class Time
{
  private int hour;
  private int min;
  
  public Time(int hour, int min)
  {
    this.hour = hour;
    this.min = min;
  }
  public int minutesUntil(Time other)
  {
    int total = (this.hour*60)+this.min;
    int otherTot = (other.getHour()*60)+other.getMin();
    return otherTot-total;
  }
  public int getHour()
  {
    return hour;
  }
  public int getMin()
  {
    return min;
  }
}
